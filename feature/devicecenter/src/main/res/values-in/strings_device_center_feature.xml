    <string name="device_center_list_view_item_status_blocked">Terblokir</string>
    <!-- The title of the Device Center feature -->
    <string name="device_center_top_app_bar_title">Pusat perangkat</string>
    <!-- The name of one of the Device Center Bottom Sheet items which takes the User to the Camera uploads feature when clicked -->
    <string name="device_center_bottom_sheet_item_camera_uploads">Unggahan camera</string>
