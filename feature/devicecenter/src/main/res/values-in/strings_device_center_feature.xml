    <string name="device_center_list_view_item_status_unknown_status">Status tidak diketahui</string>
    <!-- The name of the Device if the SDK cannot retrieve the name for that Device -->
    <string name="device_center_list_view_item_title_unknown_device">Perangkat tidak dikenal</string>
    <!-- The name of the Device or Folder Status which signifies that it is Up to Date -->
    <string name="device_center_list_view_item_status_up_to_date">Terkini</string>
